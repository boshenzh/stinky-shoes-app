{
  "name": "maptiler-maplibre-webapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "node server.js",
    "dev:vite": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:vite\"",
    "build": "vite build",
    "vercel-build": "vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node server.js",
    "db:populate": "node scripts/populate_database.js",
    "db:populate:enrich": "node scripts/populate_database.js --enrich",
    "db:enrich-local": "node scripts/enrich_local_db.js",
    "db:copy-to-neon": "node scripts/copy_local_to_neon.js",
    "db:show": "node scripts/show_database.js",
    "db:show:neon": "node scripts/show_database.js --neon",
    "db:clear": "node scripts/clear_database.js",
    "db:clear-city-state": "node scripts/clear_city_state.js"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pmtiles": "^4.3.0",
    "supercluster": "^8.0.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "vite": "^7.1.12"
  }
